{
  "manifest_version": 2,
  "name": "Newt",
  "version": "0.1.0.2",
  "description": "Find out where you came from, and where you're going",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "ico/icon16.png",
    "48": "ico/icon48.png",
    "128": "ico/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "history",
    "storage",
    "geolocation",
    "identity",
    "chrome://favicon/*"
  ],
  "background": {
    "scripts": [
      "js/jquery-2.1.3.min.js",
      "js/require.min.js",
      "js/background.js"
    ],
    "persistent": true
  }
}
